scale 2

class Persona{
      str nombre
      str nacimiento
      int rut
}

class Ensenador{
      void ensenar()
}

class Alumno{

      int conocimiento
      str[] ramos

      void estudiar()
}

'extends Ensenador, persona
class Profesor{
      int seccion
}

'extends Ensenador, persona
class Ayudante{
      int seccion
      int conocimiento

}
Persona --> Alumno
Persona --> Ayudante
Persona --> Profesor
Ensenador --> Ayudante
Ensenador --> Profesor
