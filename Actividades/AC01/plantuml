scale 2

class Persona{
      str nombre
      str nacimiento
      int rut
}

class Ensenador{
      int min_val
      int max_val

      int seccion

      void ensenar(Alumno alumno)
}

class Alumno{

      int conocimiento
      str[] ramos

      void estudiar()
}

'extends Ensenador, persona
class Profesor{
}

'extends Ensenador, alumno
class Ayudante{

}
Persona <-- Alumno
Persona <-- Profesor
Ensenador <-- Ayudante
Ensenador <-- Profesor
Alumno <-- Ayudante
